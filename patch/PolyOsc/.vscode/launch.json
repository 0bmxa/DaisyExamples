    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/PolyOsc.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32h7x.cfg",
            ],
            "armToolchainPath": "/home/${env:USER}/Developer/gcc-arm-none-eabi-9-2019-q4-major/bin/",
            "svdFile": "/home/${env:USER}/STMicroelectronics/svd/STM32H750x.svd",
            "openOCDLaunchCommands" : ["init", "reset init"],
            "runToMain" : true,
        }
    ]
}